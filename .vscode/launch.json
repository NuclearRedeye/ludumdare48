{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceRoot}/out/debug",
      "presentation": {
        "hidden": false,
        "group": "",
        "order": 1
      },
      "userDataDir": "${workspaceRoot}.vscode/chrome",
      "runtimeArgs": [
        "--disable-infobars --disable-session-crashed-bubble"
      ],
    }
  ]
}
